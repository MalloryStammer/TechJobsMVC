 @{
        //TODO DONE #1: Use a loop to display job results in a table,
        //from Viewbag.jobs (method inside listcontroller).
    }

<div class="container body-content">

    <h1>@ViewBag.title</h1>
    <style>
    tr:nth-child(even) {background-color: #f2f2f2;}
    </style>
    @foreach (var job in ViewBag.jobs)
    {
        <table class="job-listing">
            <tr>
                <td>ID: @job.Id</td>
            </tr>
            <tr>
                <td>Name: @job.Name</td>
            </tr>
            <tr>
                <td>Employer: @job.Employer</td>

            </tr>
            <tr>
                <td>Location: @job.Location</td>
            </tr>
            <tr>
                <td>Position Type: @job.PositionType</td>
            </tr>
            <tr>
                <td>Skill: @job.CoreCompetency</td>
            </tr>
        </table>
        <br />
        <br />
    }
</div>